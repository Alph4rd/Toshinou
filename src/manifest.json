{
  "manifest_version": 2,
  "name": "Toshinou",
  "version": "1.0",

  "description": "Cheat & Tool for DarkOrbit",

  "content_scripts": [
    {
      "matches": ["*://*.darkorbit.com/indexInternal.es?action=internalMapRevolution"],
      "js": [
        "js/libs/jquery-3.2.1.min.js",
        "js/libs/jquery-ui.min.js",
        "js/libs/konva.min.js",
        "js/libs/md5.min.js",
        "js/safety/SafetyChecker.js",
        "js/safety/JavaScriptChecker.js",
        "js/utils/ResourcesManager.js",
        "js/utils/Injector.js",
        "js/utils/Variables.js",
        "js/utils/Settings.js",
        "js/utils/math/Vector2D.js",
        "js/utils/math/MathUtils.js",
        "js/utils/WebClient.js",
        "js/utils/BoxType.js",
        "js/WindowFactory.js",
        "js/windows/Minimap.js",
        "js/windows/AttackWindow.js",
        "js/windows/CollectingWindow.js",
        "js/windows/AutolockWindow.js",
        "js/objects/Movable.js",
        "js/objects/Box.js",
        "js/objects/Hero.js",
        "js/objects/Ship.js",
        "js/handlers/BoxInitHandler.js",
        "js/handlers/HandlersManager.js",
        "js/handlers/ShipAttackHandler.js",
        "js/handlers/ShipCreateHandler.js",
        "js/handlers/HeroPositionUpdateHandler.js",
        "js/handlers/AssetRemovedHandler.js",
        "js/handlers/HeroInitHandler.js",
        "js/handlers/MovementDoneHandler.js",
        "js/handlers/ShipMoveHandler.js",
        "js/handlers/ShipDestroyedHandler.js",
        "js/handlers/ShipRemovedHandler.js",
        "js/worker.js",
        "js/Api.js"
      ],
      "run_at": "document_start"
    }
  ],

  "web_accessible_resources": [
    "res/injectables/*"
  ],

  "permissions": [
    "<all_urls>"
  ]
}
